{
    "name": "razorpay/subscriptions-magento-plugin",
    "description": "Razorpay Magento 2.X plugin for accepting recurring payments.",
    "version": "1.0.0",
    "require": {
        "php": "~5.6.0|~7.0|~8.0",
        "razorpay/razorpay": "2.*",
        "razorpay/magento": "4.*"
    },
    "keywords": [
        "razorpay",
        "magento",
        "payments",
        "india",
        "payment gateway",
        "wallets",
        "netbanking",
        "recurring"
    ],
    "authors": [
        {
            "name": "Team Razorpay",
            "homepage": "https://razorpay.com/about/",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/razorpay/subscriptions-magento-plugin",
        "issues": "https://razorpay.com/support/",
        "rss": "https://github.com/razorpay/subscriptions-magento-plugin/releases.atom",
        "docs": "https://docs.razorpay.com/v1/page/integrating-your-magento-store-with-razorpay"
    },
    "homepage": "https://razorpay.com",
    "suggest": {
        "magento/module-checkout-agreements": "100.0.*"
    },
    "type": "magento2-module",
    "license": ["MIT"],
    "autoload": {
        "files": ["registration.php"],
        "psr-4": {
            "Razorpay\\Subscription\\": ""
        }
    }
}
